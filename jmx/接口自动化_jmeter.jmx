            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="获取数据" enabled="true">
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">import org.apache.poi.openxml4j.exceptions.InvalidFormatException;
import org.apache.poi.ss.usermodel.WorkbookFactory;
import org.apache.poi.xssf.usermodel.*;

import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.io.IOException;

String file = vars.get(&quot;path&quot;);
FileInputStream ExcelFile = null;

try {
	ExcelFile = new FileInputStream(file);

} catch (FileNotFoundException e) {
	e.printStackTrace();
}
XSSFWorkbook excelWBook = null;
try {
	excelWBook = (XSSFWorkbook) WorkbookFactory.create(ExcelFile);
} catch (IOException e) {
	e.printStackTrace();
} catch (InvalidFormatException e) {
	e.printStackTrace();
}
XSSFSheet excelWSheet = excelWBook.getSheet(&quot;test&quot;);

String num1 = vars.get(&quot;num&quot;);
int num = Integer.valueOf(num1).intValue();

if(((String)excelWSheet.getRow(num).getCell(1).getStringCellValue()).equals(&quot;YES&quot;)){
	vars.put(&quot;run&quot;,&quot;YES&quot;);
	vars.put(&quot;case_name&quot;,(String)excelWSheet.getRow(num).getCell(2).getStringCellValue());
	vars.put(&quot;interface_name&quot;,(String)excelWSheet.getRow(num).getCell(3).getStringCellValue());
	vars.put(&quot;url&quot;,(String)excelWSheet.getRow(num).getCell(4).getStringCellValue());
	vars.put(&quot;method&quot;,(String)excelWSheet.getRow(num).getCell(5).getStringCellValue());
	vars.put(&quot;scenes&quot;,(String)excelWSheet.getRow(num).getCell(6).getStringCellValue());
	vars.put(&quot;auth_app_mark&quot;,(String)excelWSheet.getRow(num).getCell(7).getStringCellValue());
	vars.put(&quot;user_id_mark&quot;,(String)excelWSheet.getRow(num).getCell(8).getStringCellValue());
	vars.put(&quot;token_mark&quot;,(String)excelWSheet.getRow(num).getCell(9).getStringCellValue());
	
	if(excelWSheet.getRow(num).getCell(10)!=null){
		vars.put(&quot;request&quot;,(String)excelWSheet.getRow(num).getCell(10).getStringCellValue());
	}else{
		vars.put(&quot;request&quot;,&quot;&quot;);
	}
	
	String response = (String)excelWSheet.getRow(num).getCell(11).getStringCellValue();
	vars.put(&quot;response&quot;,response);
	//中文进行utf8编码
	
	StringBuffer output = new StringBuffer();
	for (int i = 0; i &lt; response.length(); i++)
	{
		if(!(response.charAt(i) &gt;= 0x4E00 &amp;&amp;  response.charAt(i) &lt;= 0x9FA5)){
			output.append(response.charAt(i));
		}else{
			output.append(&quot;\\u&quot; +Integer.toString(response.charAt(i), 16));
		}
	}
	
	vars.put(&quot;response_utf8&quot;,output.toString());
}else{
	vars.put(&quot;run&quot;,&quot;NO&quot;);
	vars.put(&quot;case_name&quot;,&quot;&quot;);
	vars.put(&quot;interface_name&quot;,&quot;&quot;);
	vars.put(&quot;url&quot;,&quot;&quot;);
	vars.put(&quot;method&quot;,&quot;&quot;);
	vars.put(&quot;scenes&quot;,&quot;&quot;);
	vars.put(&quot;auth_app_mark&quot;,&quot;NO&quot;);
	vars.put(&quot;user_id_mark&quot;,&quot;NO&quot;);
	vars.put(&quot;request&quot;,&quot;&quot;);
	vars.put(&quot;response&quot;,&quot;&quot;);
	vars.put(&quot;response_utf8&quot;,&quot;&quot;);
}



</stringProp>
